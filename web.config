<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="%PYTHON_PATH%\python.exe"
      arguments="%PYTHON_PATH%\Scripts\gunicorn.exe --bind=0.0.0.0:%HTTP_PLATFORM_PORT% --timeout 600 your_project_name.wsgi:application"
      stdoutLogEnabled="true"
      stdoutLogFile=".\logs\python.log"
      startupTimeLimit="60"
      processesPerApplication="16">
      <environmentVariables>
        <environmentVariable name="PYTHON_PATH" value="D:\home\Python39" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration>
